{
  "name": "dottor-tilde",
  "displayName": "dottor.vs-code.tilde",
  "description": "Inserimento di caratteri speciali (tilde, backtick) tramite command di Visual Studio Code",
  "version": "0.0.1",
  "author": {
    "name": "Andrea Dottor",
    "url": "https://www.dottor.net"
  },
  "engines": {
    "vscode": "^1.55.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:dottor.tilde.insert",
    "onCommand:dottor.backtick.insert"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dottor.tilde.insert",
        "title": "Insert tilde (~)"
      },
      {
        "command": "dottor.backtick.insert",
        "title": "Insert backtick (`)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.55.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  }
}
